<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-11 offset-md-1">
      <h1>タグ一覧・追加</h1>
      <%= form_with model: @tag, url: admin_tags_path, method: :post do |f| %>
        <p>タグ名</p>
        <%= f.text_field :name %>
        <%= f.submit '新規投稿' %>
      <% end %>
    </div>

  </div>
</div>
